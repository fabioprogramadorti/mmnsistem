@model List<MultiSis.Administrativo.Models.SolicitacaoSaqueModel>

<div id="dados">
    <table class="table table-striped table-bordered table-hover" style="font-size: 11px !important; margin-top: 0px !important;">
        <tr>
            <th>Feito</th>
            <th>Nome</th>
            <th>E-mail</th>
            <th>CPF ou CNPJ</th>
            <th>Tipo conta</th>
            <th>Nº Cartão</th>
            <th>Bandeira</th>
            <th>Empresa</th>
            <th style="text-align: right;">Nº banco</th>
            <th>Banco</th>
            <th style="text-align: right;">Agência</th>
            <th style="text-align: right;">Conta</th>
            <th style="text-align: right;">Valor solicitado</th>
            <th style="text-align: right;">Tarifa</th>
            <th style="text-align: right;">Valor líquido</th>
            <th style="text-align: right;">Data solicitação</th>
        </tr>
        @if (Model.Count() > 0)
        {
            foreach (var s in Model)
            {
                <tr>
                    <td><input type="checkbox" /></td>
                    <td>@s.Nome</td>
                    <td>@s.Email</td>
                    <td>@s.CPFCNPJ</td>
                    <td>@s.TipoConta</td>
                    <td>@s.NumeroCartao</td>
                    <td>@s.Bandeira</td>
                    <td>@s.Empresa</td>
                    <td style="text-align: right;">@s.NumeroBanco</td>
                    <td>@s.NomeBanco</td>
                    <td style="text-align: right;">@s.Agencia</td>
                    <td style="text-align: right;">@s.Conta</td>
                    <td style="text-align: right;">R$ @String.Format("{0:###,###,###0.00}", s.ValorSolicitado)</td>
                    <td style="text-align: right;">R$ @String.Format("{0:###,###,###0.00}", s.ValorTarifa)</td>
                    <td style="text-align: right;">R$ @String.Format("{0:###,###,###0.00}", s.ValorLiquido)</td>
                    <td style="text-align: right;">@s.DataSolicitacao.ToString("dd/MM/yyyy HH:mm")</td>
                </tr>
            }
        }
        else
        {
            <tr>
                <td colspan="12" style="padding: 2em;">
                    Nenhum registro encontrado
                </td>
            </tr>
        }
    </table>
</div>
<div class="row">
    <div class="col-lg-12">
        <button class="btn btn-success" id="btnImprimir" value="Imprimir" title="Imprimir" onclick="javascript:ImprimirObjeto('dados', 'Relatório de solicitações de saque');">Imprimir</button>
    </div>
</div>
